@model BookShelf.Models.BookShelf

@{
    ViewBag.Title = "Edit book";
}
<h2>Edit book</h2>

<div class="jumbotron">

    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <div id="edit-boxes"></div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="button" id="edit-submit" value="Update" class="btn btn-default" />
                <input type="button" id="edit-cancel" value="Cancel" class="btn btn-default" />
            </div>
        </div>
    </div>
</div>